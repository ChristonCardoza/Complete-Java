package christu.deadman.springtutorials;

public class Student {
	
	private int  studentId;
	private MathCheat MathCheat;
	

	public void setMathCheat(MathCheat mathCheat) {
		this.MathCheat = mathCheat;
		System.out.println("The MathCheat Object is created from setter");
	}

	public void  setstudentId(int studentId) {
		this.studentId = studentId;
		System.out.println("The setter method for ID is created");
	}
	
	public void cheating() {
		
		MathCheat.mathCheat();
		System.out.println("The student Id is : "+studentId);
	}

}
